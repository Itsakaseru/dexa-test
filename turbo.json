{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "start": {
      "persistent": true,
      "cache": false
    },
    "build": {
      "outputs": ["build/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
